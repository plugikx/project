import numpy as np

# Определим комплексные числа
z1 = 2 + 1j
z2 = 2 - 2j

# Вычислим сумму, разность, произведение и частное
sum = z1 + z2
difference = z1 - z2
product = z1 * z2
quotient = z1 / z2

# Выведем результаты
print("Сумма:", sum)
print("Разность:", difference)
print("Произведение:", product)
print("Частное:", quotient)
