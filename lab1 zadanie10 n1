import cmath

# Комплексные числа
z1 = complex(2, 1)
z2 = complex(2, -2)
z3 = complex(-1, 2)

# Сумма первых двух чисел
sum_of_two = z1 + z2

# Разность первых двух чисел
diff_of_two = z1 - z2

# Произведение первых двух чисел
prod_of_two = z1 * z2

# Частное первых двух чисел
quotient_of_two = z1 / z2

# Четвертая степень третьего числа
z3_fourth = pow(z3, 4)

# Корень третьей степени третьего числа
z3_cube_root = cmath.sqrt(z3)**3

# Вывод результатов
print("Сумма первых двух чисел:", sum_of_two)
print("Разность первых двух чисел:", diff_of_two)
print("Произведение первых двух чисел:", prod_of_two)
print("Частное первых двух чисел:", quotient_of_two)
print("Четвертая степень третьего числа:", z3_fourth)
print("Корень третьей степени третьего числа:", z3_cube_root)
